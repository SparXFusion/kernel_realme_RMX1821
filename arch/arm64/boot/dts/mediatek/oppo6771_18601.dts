/* FINGERPRINT start */
&spi0 {
		#address-cells = <1>;
		#size-cells = <0>;
       	finger_spi@0 {
		compatible = "mediatek,fingerspi-fp";
        reg = <0>;
		spi-id = <0>;
		spi-irq = <22>;
		spi-reg = <0x1100a000>;
		spi-max-frequency = <8000000>;
		netlink-event = <30>;
		status = "okay";
        };
};
&finger_fp {
	pinctrl-names = "default", "rst-low", "rst-high",
	                "irq-init", "spi-default";
	pinctrl-0 = <&silfp_default>;
	pinctrl-1 = <&silfp_rst_low>;
	pinctrl-2 = <&silfp_rst_high>;
	pinctrl-3 = <&silfp_irq>;
	pinctrl-4 = <&fingerprint_pins_default>;
	status = "okay";
};

&pio {
	silfp_default: silfpdefault {
	};
	silfp_rst_low: state_reset_low {
		pins_cmd_dat {
			pins = <PINMUX_GPIO158__FUNC_GPIO158>;
			slew-rate = <1>;
			output-low;
		};
	};
	silfp_rst_high: state_reset_high {
		pins_cmd_dat {
			pins = <PINMUX_GPIO158__FUNC_GPIO158>;
			slew-rate = <1>;
			output-high;
		};
	};
	silfp_irq: fp_eint@22 {
		pins_cmd_dat {
			pins = <PINMUX_GPIO22__FUNC_GPIO22>;
			slew-rate = <0>;
			bias-disable;
		};
	};
